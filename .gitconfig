[user]
	email = accurso.alan@gmail.com
	name = aaccurso
[credential]
helper = store
[color]
ui = auto
[push]
default = simple
[diff]
algorithm = patience
[core]
editor = vim
	excludesfile = /Users/alan/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[merge]
	tool = diffmerge
[mergetool "diffmerge"]
	cmd = diffmerge --merge\n--result=$MERGED $LOCAL $BASE $REMOTE
	trustExitCode = true
[alias]
	gol = log --graph --pretty=format:'%Cred%h%Creset - %C(blue)%an%Creset%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative -10
[status]
	submoduleSummary = true
